package patterns;
import java.util.*;

public class pattern1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int r=sc.nextInt();
		int c=sc.nextInt();
		for(int i=0;i<c;i++)
		{
			for(int j=0;j<r;j++)
			{
				if(j==0||j==(r-1)||i==0||i==(c-1))
					System.out.print("*");
				else
					System.out.print(" ");
			}
			System.out.println();
		}
		

	}

}
