import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int n;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        int sq=n*n;
        int temp,rev=0;
        while(sq>0)
        {
            int  rem=sq%10;
            rev+=rem;
            sq=sq/10;
        }
        if(rev==n)
        System.out.println("this is a neon num"+ n);
        else
        System.out.println("its not a neon num");
        
    }
}
